{
  "api_base_url": "https://YOUR-VERCEL-APP.vercel.app/api",
  "endpoints": {
    "creators": {
      "url": "/creators",
      "method": "GET",
      "parameters": {
        "page": "1",
        "size": "15"
      },
      "description": "Get all creators in your agency"
    },
    "creator_earnings": {
      "url": "/creators/{creatorUuid}/earnings",
      "method": "GET",
      "parameters": {
        "startDate": "2025-01-01",
        "endDate": "2025-12-31",
        "maxPages": "10"
      },
      "description": "Get earnings for a specific creator"
    },
    "creator_followers": {
      "url": "/creators/{creatorUuid}/followers",
      "method": "GET",
      "parameters": {
        "page": "1",
        "size": "50"
      },
      "description": "Get followers for a specific creator"
    },
    "creator_subscribers": {
      "url": "/creators/{creatorUuid}/subscribers",
      "method": "GET",
      "parameters": {
        "page": "1",
        "size": "50"
      },
      "description": "Get subscribers for a specific creator"
    },
    "message_analytics": {
      "url": "/message-analytics",
      "method": "GET",
      "parameters": {
        "startDate": "2025-01-01",
        "endDate": "2025-12-31",
        "maxPages": "5"
      },
      "description": "Get overall message analytics across all creators"
    },
    "creator_message_volume": {
      "url": "/creators/{creatorUuid}/message-volume",
      "method": "GET",
      "parameters": {
        "startDate": "2025-01-01",
        "endDate": "2025-12-31",
        "maxPages": "5"
      },
      "description": "Get message volume for a specific creator"
    },
    "fan_engagement": {
      "url": "/fan-engagement",
      "method": "GET",
      "parameters": {
        "startDate": "2025-01-01",
        "endDate": "2025-12-31",
        "maxPages": "3",
        "minMessages": "1"
      },
      "description": "Get fan engagement metrics across all creators"
    }
  },
  "powerbi_setup": {
    "data_sources": [
      {
        "name": "Creators",
        "url": "https://YOUR-VERCEL-APP.vercel.app/api/creators?page=1&size=15",
        "refresh_frequency": "Daily"
      },
      {
        "name": "Creator_Earnings",
        "url": "https://YOUR-VERCEL-APP.vercel.app/api/creators/{creatorUuid}/earnings?startDate=2025-01-01&endDate=2025-12-31&maxPages=10",
        "refresh_frequency": "Daily",
        "note": "Replace {creatorUuid} with actual creator UUIDs"
      },
      {
        "name": "Creator_Subscribers",
        "url": "https://YOUR-VERCEL-APP.vercel.app/api/creators/{creatorUuid}/subscribers?page=1&size=50",
        "refresh_frequency": "Daily",
        "note": "Replace {creatorUuid} with actual creator UUIDs"
      },
      {
        "name": "Message_Analytics",
        "url": "https://YOUR-VERCEL-APP.vercel.app/api/message-analytics?startDate=2025-01-01&endDate=2025-12-31&maxPages=5",
        "refresh_frequency": "Daily",
        "description": "Overall message metrics across all creators"
      },
      {
        "name": "Creator_Message_Volume",
        "url": "https://YOUR-VERCEL-APP.vercel.app/api/creators/{creatorUuid}/message-volume?startDate=2025-01-01&endDate=2025-12-31&maxPages=5",
        "refresh_frequency": "Daily",
        "note": "Replace {creatorUuid} with actual creator UUIDs for individual creator message data"
      },
      {
        "name": "Fan_Engagement",
        "url": "https://YOUR-VERCEL-APP.vercel.app/api/fan-engagement?startDate=2025-01-01&endDate=2025-12-31&maxPages=3&minMessages=1",
        "refresh_frequency": "Daily",
        "description": "Fan engagement metrics and message activity"
      }
    ],
    "recommended_visualizations": [
      "Total subscribers by creator",
      "Earnings trends over time",
      "Creator performance comparison",
      "Monthly revenue dashboard",
      "Message volume by creator",
      "Fan engagement scores",
      "Message sent vs received trends",
      "Top engaged fans by creator",
      "Daily message activity patterns",
      "Creator response rates"
    ]
  }
}
